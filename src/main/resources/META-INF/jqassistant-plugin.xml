<jqa-plugin:jqassistant-plugin xmlns:jqa-plugin="http://www.buschmais.com/jqassistant/core/plugin/schema/v1.4" name="Java DDD">
    <description>Provides shared rules for jMolecules.</description>
    <model>
        <class>org.jqassistant.contrib.plugin.jmolecules.descriptor.JMoleculesDescriptor</class>
        <class>org.jqassistant.contrib.plugin.jmolecules.descriptor.architecture.ArchitectureDescriptor</class>
        <class>org.jqassistant.contrib.plugin.jmolecules.descriptor.architecture.layered.LayeredDescriptor</class>
        <class>org.jqassistant.contrib.plugin.jmolecules.descriptor.architecture.layered.LayerDescriptor</class>
        <class>org.jqassistant.contrib.plugin.jmolecules.descriptor.architecture.onion.OnionDescriptor</class>
        <class>org.jqassistant.contrib.plugin.jmolecules.descriptor.architecture.onion.RingDescriptor</class>
        <class>org.jqassistant.contrib.plugin.jmolecules.descriptor.ddd.BoundedContextDescriptor</class>
        <class>org.jqassistant.contrib.plugin.jmolecules.descriptor.ddd.DDDDescriptor</class>
        <class>org.jqassistant.contrib.plugin.jmolecules.descriptor.ddd.ModuleDescriptor</class>
    </model>
    <rules>
        <resource>jmolecules-ddd.xml</resource>
        <resource>jmolecules-event.xml</resource>
        <resource>jmolecules-layered.xml</resource>
        <resource>jmolecules-onion-simplified.xml</resource>
        <resource>jmolecules-onion-classical.xml</resource>
    </rules>
</jqa-plugin:jqassistant-plugin>
